<rdf:Description rdf:about="http://sr#o_1e0acae5">
	<sr:name>Repository</sr:name>
	<sr:header>require &apos;model_loader&apos;
require &apos;model_writer&apos;
require &apos;ostruct&apos;


layout &apos;admin&apos;
  
before_filter :login_required

  def login_required
    if session[:user]
      return true
    end
    flash[:warning]=&apos;Please login to continue&apos;
    session[:return_to]=request.request_uri
    redirect_to :controller =&gt; :new_wiki_user, :action =&gt; :login
    return false
  end
  

  def select
    @repos = Dir[&quot;#{RAILS_ROOT}/db/*.rdf&quot;].map { |f| Pathname.new(f).basename.to_s[0..-5] }.sort
  end

  def set
    reset_session
    cookies.delete &quot;breadcrumb&quot;
    session[&apos;repo&apos;] = &quot;db/#{params[:id]}.rdf&quot;
    set_repo
    redirect_to :controller =&gt; &apos;nav_class&apos;, :action =&gt; &apos;list&apos; and return
  end
  
  def import
    @db = OpenStruct.new(@params[&apos;db&apos;])
    if request.post?
      begin
        if @db.script.blank?
          modelo = YAML.load(@db.file)
        else
          modelo = YAML.load(@db.script)
        end
      rescue =&gt; ex
        flash[&apos;notice&apos;] = &quot;Invalid database script, not valid YAML: #{ex}&quot;
        return
      end

      begin
        ModelObserver.suspend
        SemanticRecord::Base.connection.clear_repository if @db.clear == &apos;1&apos;
        ModelLoader.new(modelo).load
        ModelObserver.resume
      rescue =&gt; ex 
        ModelObserver.resume
        flash[&apos;notice&apos;] = &quot;Error while importing script: #{ex}. 
        This might have left the repository in an inconsistent state.&quot;
        return
      end
      
      flash[&apos;notice&apos;] = &quot;Script imported succesfully!&quot;
    end
  end
  
  def export
    
    if request.post?
      
      write_data_options = Hash.new(false)
      write_data_options[:classes] = true if request.params[&quot;classes&quot;] == &quot;on&quot;
      write_data_options[:contexts] = true if request.params[&quot;contexts&quot;] == &quot;on&quot;
      write_data_options[:indices] = true if request.params[&quot;indices&quot;] == &quot;on&quot;
      write_data_options[:landmarks] = true if request.params[&quot;landmarks&quot;] == &quot;on&quot;
      write_data_options[:views] = true if request.params[&quot;views&quot;] == &quot;on&quot;
      write_data_options[:nodes] = true if request.params[&quot;nodes&quot;] == &quot;on&quot;
      
      p &apos;request.params[&quot;classes&quot;]&apos;, request.params[&quot;classes&quot;], request.params[&quot;classes&quot;].class
      p &apos;request.params.default&apos;, request.params.default
      p &quot;write_data_options&quot;, write_data_options
      p &quot;request.params&quot;, request.params
      
      begin
        @file_name = &quot;#{SemanticRecord::Base.repository[&apos;repository&apos;]}-#{Time.now.strftime(&quot;%Y-%m-%dT%H%M%S&quot;)}.tmp.yml&quot;
        path = &quot;#{RAILS_ROOT}/public/#{@file_name}&quot;
        ModelWriter.new(path, write_data_options).write
      rescue =&gt; ex
        flash[&apos;notice&apos;] = &quot;Error while exporting repository: #{ex}.&quot;
        return
      end
      
      flash[&apos;notice&apos;] = &quot;Repository exported succesfully. Please download the YAML script below.&quot;
    end
    
  end</sr:header>
</rdf:Description>

