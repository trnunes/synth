<rdf:Description rdf:about="http://sr#o_c5d9b7a1">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>get_attachments</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>qry = &quot;
  SELECT DISTINCT id, type, name
  FROM {id} rdf:type {sr:FileAttachment}; 
            serql:directType {type}; 
            [ sr:name {name} ], 
            {link} rdf:type {sr:AttachedFile}; 
                   sr:node_id {sr:%s}; 
                   sr:target_node_id {id} 
  ORDER BY name USING NAMESPACE sr = &lt;http://sr#&gt;&quot; % 
  [self.id]

records = QueryBuilder.execute(qry, [], Node)
nodes = []
if !records.empty? &amp;&amp; records.first.is_a?(Hash)
  ids = records.collect { |rec| rec[&apos;id&apos;].strip_namespace }
  if ids.length &gt; 0
    unsorted = Node.find ids
    nodes = unsorted.sort_by { |node| ids.index(node.id) }
  end
else
  nodes = records
end
nodes</sr:code>
