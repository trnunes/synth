	<sr:header>helper :node
	
  include CrudController
  layout &apos;layouts/admin&apos;
	
	prepend_before_filter { AppModel.sync }

  crud Node
  
before_filter :login_required

  def login_required
    if session[:user]
      return true
    end
    flash[:warning]=&apos;Please login to continue&apos;
    session[:return_to]=request.request_uri
    redirect_to :controller =&gt; :new_wiki_user, :action =&gt; :login
    return false
  end
  

  def list
    @nav_classes = NavClass.find_all nil, &quot;name&quot;
    
    @nav_class = NavClass.find(@params[&quot;id&quot;]) if @params[&quot;id&quot;]
    if @nav_class 
      unsorted_nodes = eval &quot;#{@nav_class.name}.find_all&quot;
      @nodes = unsorted_nodes.sort { |a,b| a.label.to_s &lt;=&gt; b.label.to_s }
    end
    @page_title = title_for_list
    render_action &quot;list&quot;		    
  end

  def before_new
    nav_class_id = @params[&quot;nav_class_id&quot;]
    if @params[&quot;id&quot;] &amp;&amp; nav_class_id.nil?
      @nav_class = Node.find(@params[&quot;id&quot;]).nav_class
    end
    if nav_class_id || @nav_class
      @nav_class ||= NavClass.find(nav_class_id)
      @node = eval &quot;#{@nav_class.name}.new&quot;
    else
      @nav_classes = NavClass.find_all
    end
  end

  def select_class
    @page_title = &quot;Select Class&quot;
		before_new    
  end
  
  def template_for_new
    @nav_class.nil? ? &quot;select_class&quot; : &quot;edit&quot;
  end
  
  def before_edit
    @nav_class = NavClass.find(@node.nav_class.id)
  end

  def before_delete
	end
	    
  def create
    before_new
    @node.attributes = @params[&quot;node&quot;]
    if @node.save
      set_notice notice_for_create
      redirect_after_create
    else
      treat_errors @node, &quot;new&quot;
    end
  end

  def update
    @node = Node.find(@params[&quot;node&quot;][&quot;id&quot;])
    @nav_class = @node.nav_class
    @node.attributes = @params[&quot;node&quot;]
		if @node.save
		  set_notice notice_for_update
      redirect_after_update
		else
		  treat_errors @node, &quot;edit&quot;
		end
	end

	def new_link
	  link_type = @params[&quot;type&quot;]
	  @link = Link.find_first [&quot;name = ?&quot;, link_type]
	  @nodes = eval &quot;#{@link.target_nav_class.name}.find_all&quot;
	  @node = Node.find(@params[&quot;id&quot;])
	  existing_links = eval &quot;#{link_type}.find_all_by_node @node.id&quot;
	  @nodes.reject! { |n| existing_links.find { |l| l.target_node_id == n.id } } 
	  @nodes.sort! { |a,b| a.label &lt;=&gt; b.label }
	  @page_title = &quot;Add Links&quot;
	end

	def add_link
		count = 0
		@params[&quot;target&quot;].each { |id|
			NodeLink.create &quot;node_id&quot; =&gt; @params[&quot;id&quot;], &quot;target_node_id&quot; =&gt; id, &quot;type&quot; =&gt; @params[&quot;type&quot;]
			count += 1
		}	      
		set_notice &quot;#{count} link(s) added.&quot;
		redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @params[&quot;id&quot;]
	end

	def remove_link
	  count = 0
	  link = nil
	  link_method = @params[&quot;link_method&quot;]
	  @params[link_method].each { |id|
	    link = NodeLink.destroy id
	    count += 1
	  }
		set_notice &quot;#{count} link(s) removed.&quot;
		redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; link.node_id
	end

	def link_up
	  link_action { |link, notice| link.move_higher; notice &lt;&lt; &quot;moved up.&quot; }
	end

	def link_up5
	  5.times { link_up }
	end

	def link_down
	  link_action { |link, notice| link.move_lower; notice &lt;&lt; &quot;moved down.&quot; }
	end	  
	
	def link_down5
	  5.times { link_down }
	end	  

	def link_to_top
	  link_action { |link, notice| link.move_to_top; notice &lt;&lt; &quot;moved to top.&quot; }
	end	  
	  
	def link_to_bottom
	  link_action { |link, notice| link.move_to_bottom; notice &lt;&lt; &quot;moved to bottom.&quot; }
	end	  
	
	def link_action
	  link = NodeLink.find(@params[&quot;id&quot;])
	  notice = &quot;Link &quot;
	  yield(link, notice)
	  set_notice notice
	  redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; link.node_id
	end
		  	
	def notice_for_create() &quot;#{@node.type.to_s} created. Now add links to it.&quot; end
	def form_action_for_create() url_for :action =&gt; &quot;create&quot;, :id =&gt; @params[&quot;id&quot;], :params =&gt; { &quot;nav_class_id&quot; =&gt; @params[&quot;nav_class_id&quot;] } end
	def form_action_for_update() url_for :action =&gt; &quot;update&quot;, :id =&gt; @params[&quot;id&quot;] end
	 
	def redirect_after_create() redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @node.id end
	def redirect_after_update() do_return_to :action =&gt; &quot;list&quot;, :id =&gt; @node.nav_class.id end
	def redirect_after_delete() redirect_after_update end
		    
	def title_for_new() &quot;New #{@node.class.name}&quot; end    
	def title_for_edit() &quot;Edit #{@node.class.name}&quot; end    
	def title_for_list
	  if @nav_class
	    &quot;#{@nav_class.name} List&quot; 
		else
		  &quot;Node List&quot;
		end
	end</sr:header>
