	<sr:code>attachmentsId = context # renaming default first parameter

attachmentsId.each{ |id|
  attach = FileAttachment.find(id)
  attach.node_links.each_value{ |nodeLink|
    nodeLink[:link].destroy
  }
  repositoryPath = HyperDEConfiguration.find_by_parameter &quot;file_attachment_repository_path&quot;
  repositoryPath = repositoryPath.value
  File.delete(repositoryPath + &quot;/&quot; + attach.id + &quot;_&quot; + attach.name)
  attach.destroy
}</sr:code>
