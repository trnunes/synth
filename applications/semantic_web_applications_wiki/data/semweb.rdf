<rdf:Description rdf:about="http://sr#o_f97d33f7">
	<sr:code>wikiText = text # renaming default first parameter
self.wikiText = wikiText 

self.destroy_strictly_annotated_links
annotations = self.get_annotations(nil, nil, self.wikiText)

#removo as anotações wiki_see_also que cujos target_node não existem
annotations = annotations.reject{|v| v[:objectProperty] and v[:value].class == String}

annotations.each { |annotation|
  if annotation[:objectProperty]
    navLink = Link.find_by_name(annotation[:propertyName])
    
    unless navLink
      navLink = Link.new
      navLink.name = annotation[:propertyName]
      wikiPageNavClass = NavClass.find_by_name(&quot;WikiPage&quot;)
      navLink.nav_class_id = wikiPageNavClass.id
      navLink.target_nav_class_id = wikiPageNavClass.id
      navLink.save
    end
    
    if annotation[:value].class == String
      targetNavClass = NavClass.find(navLink[&quot;target_nav_class_id&quot;])
      targetNavClass = WikiPage if targetNavClass.name == &quot;AbstractWikiPage&quot;
      resourceValue = Object.const_get(targetNavClass.name).new
      resourceValue.wikiLabel = annotation[:value]
      resourceValue.save
      annotation[:value] = resourceValue 
    end
    
    # if the link does not existe, creates it
    if !NodeLink.find_by_values self.id, 
                                annotation[:value].id, 
                                annotation[:propertyName]
      NodeLink.create &quot;node_id&quot; =&gt; self.id,
                      &quot;target_node_id&quot; =&gt; annotation[:value].id,
                      &quot;type&quot; =&gt; annotation[:propertyName]
    end
    
  else
    navAttribute = self.has_declared_datatype_property(nil, nil, annotation[:propertyName])
    unless navAttribute
      nav_attribute = NavAttribute.new
      nav_attribute.name = annotation[:propertyName]
      nav_attribute.nav_class_id = NavClass.find_by_name(self.class.name).id
      nav_attribute.data_type = &quot;String&quot;
      
      nav_attribute.link_id = &quot;None&quot;
      nav_attribute.context_id = &quot;None&quot;
      nav_attribute.type = &quot;NavAttribute&quot;
      nav_attribute.anchor_index_id = &quot;None&quot;
      nav_attribute.index_id = &quot;None&quot;
      nav_attribute.save
      self.save
    end
    attrs = HashWithIndifferentAccess.new
    attrs[annotation[:propertyName]] = annotation[:value]
    self.attributes = attrs
  end
}


assign_editor(context, controller) if HyperDEConfiguration.find_by_parameter(&quot;access_control&quot;).value == &quot;on&quot;

self.save</sr:code>
</rdf:Description>

