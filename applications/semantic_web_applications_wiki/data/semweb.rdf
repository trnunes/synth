}
.error { color: red; font-weight: bold }
&lt;/style&gt;
&lt;span class=&quot;notice&quot;&gt;&lt;%= flash[:message] %&gt;&lt;/span&gt;
&lt;span class=&quot;error&quot;&gt;&lt;%= flash[:warning] %&gt;&lt;/span&gt;
&lt;h1&gt; Create New a Wiki User&lt;/h1&gt;
&lt;br/&gt;
&lt;% form_remote_tag :url =&gt; { :controller =&gt; :new_wiki_user, :action =&gt; :signup},:update =&gt; &apos;main-copy&apos; do %&gt;
&lt;input name=&quot;op&quot; value=&quot;signup&quot; type=&quot;hidden&quot;&gt;
