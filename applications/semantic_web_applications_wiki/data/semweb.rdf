<rdf:Description rdf:about="http://sr#o_7009cc86">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>save_form_links</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>pageAtts = context # renaming default first parameter


for link_method, link in self.node_links
  oldLinks = eval(&quot;#{link[:link].name}.find_all_by_node &apos;#{self.id}&apos;&quot;)
  oldLinksMap = {}
  oldLinksSet = []
  oldLinks.each { |nodeLink|
    oldLinksMap[nodeLink[&quot;target_node_id&quot;]] = nodeLink
    oldLinksSet &lt;&lt; nodeLink[&quot;target_node_id&quot;]
  }
  newLinksSet = []
  i = 0
  size = pageAtts[link_method+&quot;_size&quot;].to_i
  while i &lt; size
    newLinksSet &lt;&lt; pageAtts[link_method+&quot;_&quot;+i.to_s]
    i = i + 1
  end
  newLinksSet.compact!
  removedLinks = oldLinksSet - newLinksSet
  createdLinks = newLinksSet - oldLinksSet


  removedLinks.each { |targetId|
    oldLinksMap[targetId].destroy
  }
  
  createdLinks.each { |targetId|
    NodeLink.create &quot;node_id&quot; =&gt; self.id,
                    &quot;target_node_id&quot; =&gt; targetId,
                    &quot;type&quot; =&gt; link[:link].name
  }
end</sr:code>
	<sr:position>0</sr:position>
</rdf:Description>

