	<sr:template>&lt;form action=&quot;&lt;%= WikiC.find_all.first.link %&gt;&quot; enctype=&quot;multipart/form-data&quot; method=&quot;post&quot;&gt;
&lt;input name=&quot;op&quot; value=&quot;save_wiki_page_form&quot; type=&quot;hidden&quot;&gt;
&lt;input name=&quot;wikiLabel&quot; value=&quot;&lt;%= @node.wikiLabel %&gt;&quot; type=&quot;hidden&quot;&gt;

&lt;div id=&quot;right-side-bar&quot;&gt;
  &lt;%= component &quot;AttachFile&quot; %&gt;
&lt;/div&gt;

&lt;% for attr in @node.get_form_edit_attributes %&gt;
  &lt;p&gt;
    &lt;label&gt;&lt;%= attr.name %&gt;&lt;/label&gt;
    &lt;% case attr.data_type.strip.downcase
       when &quot;text&quot;, &quot; html&quot;, &quot;xml&quot; %&gt;
      &lt;%= text_area &quot;node&quot;, attr.name, &quot;cols&quot; =&gt; 80 %&gt;
    &lt;% when &quot;date&quot; %&gt;
      &lt;%= date_select &quot;node&quot;, attr.name, :start_year =&gt; 1900 %&gt;
    &lt;% when &quot;time&quot; %&gt;
      &lt;%= datetime_select &quot;node&quot;, attr.name, :start_year =&gt; 1900 %&gt;
    &lt;% when &quot;datetime&quot; %&gt;
      &lt;%= datetime_select &quot;node&quot;, attr.name, :start_year =&gt; 1900 %&gt;
    &lt;% when &quot;boolean&quot; %&gt;
      &lt;%= check_box &quot;node&quot;, attr.name %&gt;
    &lt;% else %&gt;
      &lt;%= text_field &quot;node&quot;, attr.name, &quot;size&quot; =&gt; 75 %&gt;
    &lt;% end %&gt;
  &lt;/p&gt;
&lt;% end %&gt;

&lt;%= component &quot;FormEditLinks&quot; %&gt;

&lt;input type=&quot;submit&quot; value=&quot;Save&quot;/&gt;
&lt;/form&gt;</sr:template>
</rdf:Description>

