	<sr:name>FormEditLinks</sr:name>
	<sr:template>&lt;style type=&quot;text/css&quot;&gt;
&lt;!--
div.scroll {
max-height: 250px;
width: 500px;
overflow: auto;
border: 0px solid #666;
background-color: #eee;
padding: 8px;
}

 .yui-dt-even {
background-color: #fff;}

--&gt;
&lt;/style&gt; 

&lt;script language=&quot;javascript&quot;&gt;
// Define a custom format function
formatBooleanCheckbox = function(elCell, oRecord, oColumn, oData) {
  var oBool = oData;
  var inputHtml = &quot;&lt;input name=&apos;node[&quot; + 
                  oRecord.getData(&quot;field_name&quot;) + 
                  &quot;]&apos; type=&apos;checkbox&apos; value=&apos;&quot; + 
                   oRecord.getData(&quot;node_id&quot;) + &quot;&apos;&quot;;
  if (oBool == 1) {
    inputHtml = inputHtml + &quot; checked/&gt;&quot;;
  } else {
    inputHtml = inputHtml + &quot;/&gt;&quot;;
  }
  elCell.innerHTML = inputHtml;
};

toggleSelectinCellClick = function(ev, dataTable) {
  var target = YAHOO.util.Event.getTarget(ev);
  var record = dataTable.getRecord(target);
  //~ record.setData(&quot;name&quot;,(record.getData(&quot;name&quot;) + 1) % 2);
  dataTable.getRecordSet().updateRecordValue(record, &quot;linked&quot;, (record.getData(&quot;linked&quot;) + 1) % 2)
  dataTable.render();
  //~ var o = myDataTable.getTrIndex(target);
  //~ alert(o);
  //~ alert(PietschSoft.Reflection.GetProperties(o));
  //~ alert(PietschSoft.Reflection.GetMethods(o));
}

&lt;/script&gt;

&lt;% linkCnt = 0; %&gt;

&lt;% for link_method, link in @node.node_links %&gt;
  &lt;% linkCnt = linkCnt + 1; %&gt;
  &lt;h1&gt;&lt;%= link_method %&gt;&lt;/h1&gt;

  &lt;div id=&quot;tb-&lt;%= link_method %&gt;&quot; class=&quot;scroll&quot;&gt;

  &lt;% 

    existing_links = eval(&quot;#{link[:link].name}.find_all_by_node @node.id&quot;).collect{|ln| ln.target_node_id}

    navClass = NavClass.find(link[:link].target_nav_class_id) 
    navClass = eval navClass.name
    navClasses = [navClass] + navClass.subclasses
    linkData = []

    processedNodes = Set.new
    for navClass in navClasses
      for node in navClass.find_all
        unless processedNodes.include? node.id then
          processedNodes &lt;&lt; node.id
          linkData &lt;&lt; {:node =&gt; node, :linked =&gt; existing_links.include?(node.id) ? 1 : 0 }
        end
      end
    end

  %&gt;

  &lt;script language=&quot;javascript&quot;&gt;
  &lt;% linkOptionsCnt = 0; %&gt;
  linkDataArray&lt;%= linkCnt %&gt; = [
    &lt;% for data in linkData %&gt;
      {
        label:&quot;&lt;%= data[:node].label %&gt;&quot;,
        classType:&quot;&lt;%= data[:node].class.name %&gt;&quot;,
        linked:&lt;%= data[:linked] %&gt;,
        field_name:&quot;&lt;%= link_method %&gt;_&lt;%= linkOptionsCnt %&gt;&quot;,
        node_id:&quot;&lt;%= data[:node].id %&gt;&quot;,
      },
      &lt;% linkOptionsCnt = linkOptionsCnt + 1; %&gt;
    &lt;% end %&gt;
  ];

  myColumnDefs&lt;%= linkCnt %&gt; = [
      {key:&quot;linked&quot;, label:&quot;Linked&quot;, sortable:true, formatter: formatBooleanCheckbox},
      {key:&quot;label&quot;, label:&quot;Label&quot;, sortable:true},
      {key:&quot;classType&quot;, label:&quot;Class&quot;, sortable:true},
      {key:&quot;field_name&quot;, label:&quot;field_name&quot;},
      {key:&quot;node_id&quot;, label:&quot;node_id&quot;},
  ];

  // 3 fields of data are available in the RecordSet
  myDataSource&lt;%= linkCnt %&gt; = new YAHOO.util.DataSource(linkDataArray&lt;%= linkCnt %&gt;);
  myDataSource&lt;%= linkCnt %&gt;.responseType = YAHOO.util.DataSource.TYPE_JSARRAY;
  myDataSource&lt;%= linkCnt %&gt;.responseSchema = {
      fields: [&quot;label&quot;,&quot;classType&quot;,&quot;linked&quot;,&quot;field_name&quot;,&quot;node_id&quot;]
  };


  // Instantiate right away...
  myDataTable&lt;%= linkCnt %&gt; = new YAHOO.widget.DataTable(
                                            &quot;tb-&lt;%= link_method %&gt;&quot;, 
                                            myColumnDefs&lt;%= linkCnt %&gt;, 
                                            myDataSource&lt;%= linkCnt %&gt; 
                                           );

  // Enables linked selection
  myDataTable&lt;%= linkCnt %&gt;.subscribe(&quot;cellClickEvent&quot;, toggleSelectinCellClick, myDataTable&lt;%= linkCnt %&gt;); 

  myDataTable&lt;%= linkCnt %&gt;.hideColumn(myDataTable&lt;%= linkCnt %&gt;.getColumn(3))
  myDataTable&lt;%= linkCnt %&gt;.hideColumn(myDataTable&lt;%= linkCnt %&gt;.getColumn(4))

  &lt;/script&gt;

