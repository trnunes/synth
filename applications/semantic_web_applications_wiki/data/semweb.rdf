<rdf:Description rdf:about="http://sr#o_6ee074cb">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>render_object</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>if obj.class == ContextIndex
  ctxIndex = obj
  return render(ctxIndex.context)
elsif obj.class == Context
  ctx = obj
  i = 0
  text = &quot;&quot;
  while i &lt; ctx.nodes.size do
    uri = &quot;&lt;li&gt; &lt;a href=&apos;#{BASE_URL}/navigation/context/%s@%s?p=%s&apos;&gt;%s&lt;/a&gt;&lt;/li&gt;&quot; % 
            [ctx.id, i.to_s, ctx.params_values.class &lt;= Array ? ctx.params_values.join(&quot;,&quot;) : ctx.params_values, ctx.nodes[i].label]
    text = text + uri
    i = i + 1
  end
  text = &quot;&lt;ul class=&apos;wikiList&apos;&gt;&quot; + text + &quot;&lt;/ul&gt;&quot;      
  return text
elsif obj.class == Array
  return &quot;&lt;ul&gt; &lt;li/&gt;&quot; + obj.collect{|e| render(e)}.join(&quot;&lt;li/&gt;&quot;) + &quot;&lt;/ul&gt;&quot;
elsif obj.class &lt; Node
  return &quot;&lt;a href=&apos;%s&apos;&gt;%s&lt;/a&gt;&quot; % [obj.get_default_link, obj.label]
else
  return obj.to_s
end</sr:code>
	<sr:params>obj = nil</sr:params>
	<sr:position>0</sr:position>
</rdf:Description>

