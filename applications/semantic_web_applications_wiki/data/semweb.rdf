	<sr:code>flash[:message] = &quot;&quot;
flash[:warning] = &quot;&quot;
if request.post?  

  login = params[&quot;login&quot;]
  password = params[&quot;password&quot;]
  email = params[&quot;email&quot;]
  
  unless (login.empty? or password.empty? or email.empty?)

    user = WikiUser.new
    user.login = login
    user.email = email
    user.set_password(@context, nil, password)

    if user.save

      #controller.current_user = authenticate(@context, nil, user.login, password) 
      if params[&quot;is_student&quot;]
         person = Student.new
      else
         person = Person.new
      end
      person.wikiLabel = login
      person.save

      NodeLink.create &quot;node_id&quot; =&gt; user.id,
                      &quot;target_node_id&quot; =&gt; person.id,
                      &quot;type&quot; =&gt; &apos;WikiUserPerson&apos;
    
      flash[:message] = &quot;Signup successful&quot;

      #controller.params[&quot;redirect_to&quot;] = person.get_default_link
    else
      flash[:message] = &quot;Signup unsuccessful&quot;
    end  
  else
      flash[:warning] = &quot;Signup unsuccessful&quot;
  end
end
render :inline =&gt; View.find_by_name(&apos;CreateWikiUser&apos;).template</sr:code>
</rdf:Description>

