<rdf:Description rdf:about="http://sr#o_3c88f1d3">
	<sr:title>Related By</sr:title>
	<sr:query>{
  |*params|
  qry = &quot;
    SELECT DISTINCT id, type, wikiLabel
    FROM {id} rdf:type {sr:WikiPage}; 
              serql:directType {type}; 
              [ sr:wikiLabel {wikiLabel} ], 
              {link} sr:node_id {sr:%s}; 
                     rdf:type {sr:%s}; 
                     sr:target_node_id {id} 
    ORDER BY wikiLabel USING NAMESPACE sr = &lt;http://sr#&gt;&quot; % 
    params

  records = QueryBuilder.execute(qry, [], Node)
  nodes = []
  if !records.empty? &amp;&amp; records.first.is_a?(Hash)
    ids = records.collect { |rec| rec[&apos;id&apos;].strip_namespace }
    if ids.length &gt; 0
      unsorted = Node.find ids
      nodes = unsorted.sort_by { |node| ids.index(node.id) }
    end
  else
    nodes = records
  end
  nodes
}</sr:query>
	<sr:restricted>0</sr:restricted>
	<sr:name>relatedBy</sr:name>
</rdf:Description>

