<rdf:Description rdf:about="http://sr#o_9a26898d">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:position>0</sr:position>
	<sr:name>save_wiki_page</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_ae11b4a8"/>
	<sr:code>label = controller.params[&quot;wikiLabel&quot;]
node = WikiPage.find_by_wikiLabel(label)

text = controller.params[&quot;wikiText&quot;]
node.save_wiki_text(context, controller, text)

removeAttachments = []
attachmentsSize = controller.params[&quot;attachments_size&quot;].to_i
i = 0
while i &lt; attachmentsSize 
  removeAttachments &lt;&lt; controller.params[&quot;remove_attachment_&quot; + i.to_s]
  i = i + 1
end
node.remove_attachments(removeAttachments.compact)

attachments = []
attachmentsFieldsSize = controller.params[&quot;attachments_fields_size&quot;].to_i
i = 0
while i &lt; attachmentsFieldsSize
  attachments &lt;&lt; controller.params[&quot;node&quot;][&quot;attachment_&quot; + i.to_s]
  i = i + 1
end

node.save_attachments(attachments.compact)

controller.params[&quot;redirect_to&quot;] = node.get_default_link</sr:code>
</rdf:Description>

