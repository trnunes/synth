	<sr:query>{
  |*params|
  qry = &quot;SELECT * FROM {id} rdf:type {sr:WikiPage}; serql:directType {WikiPage}; [ sr:updated_at {updated_at} ] ; [ sr:wikiLabel {wikiLabel} ] USING NAMESPACE sr = &lt;http://sr#&gt;
&quot; % params

  records = QueryBuilder.execute(qry, [], Node)
  nodes = records.sort_by {|x| DateTime.parse(x[&quot;updated_at&quot;]) }.reverse 
  main = nodes.select { |v| v[&apos;wikiLabel&apos;] == &apos;Main&apos; }.first
  nodes = nodes[0..14]
  nodes &lt;&lt; main
}</sr:query>
</rdf:Description>

