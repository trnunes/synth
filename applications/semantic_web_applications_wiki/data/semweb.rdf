<rdf:Description rdf:about="http://sr#o_5b88b6f7">
	<sr:name>save_attachments</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>attachments = context # renaming default first parameter

all_files = WikiPage.find_by_wikiLabel(&quot;All Files&quot;)

attachments.each{ |attParam|
  attNode = FileAttachment.new
  attNode.name = attParam.original_filename
  attNode.save
  NodeLink.create &quot;node_id&quot; =&gt; self.id,
                  &quot;target_node_id&quot; =&gt; attNode.id,
                  &quot;type&quot; =&gt; &quot;AttachedFile&quot;

  NodeLink.create &quot;node_id&quot; =&gt; all_files.id,
                  &quot;target_node_id&quot; =&gt; attNode.id,
                  &quot;type&quot; =&gt; &quot;AttachedFile&quot;

  repositoryPath = HyperDEConfiguration.find_by_parameter &quot;file_attachment_repository_path&quot;
  repositoryPath = repositoryPath.value
  file = File.new(repositoryPath + &quot;/&quot; + attNode.id + &quot;_&quot; + attNode.name, &quot;wb&quot;)
  file.write attParam.read
  file.close
}</sr:code>
	<sr:position>0</sr:position>
</rdf:Description>

