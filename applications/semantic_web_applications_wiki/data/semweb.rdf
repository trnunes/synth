	<sr:header>helper :nav_class
  
  include CrudController
  layout &quot;layouts/admin&quot;

  crud NavClass

  before_filter :login_required

  def login_required
    if session[:user]
      return true
    end
    flash[:warning]=&apos;Please login to continue&apos;
    session[:return_to]=request.request_uri
    redirect_to :controller =&gt; :new_wiki_user, :action =&gt; :login
    return false
  end
  
  def title_for_list() &quot;Navigational Classes List&quot; end
  def title_for_new()  &quot;New Navigational Class&quot; end
  def title_for_edit() &quot;Edit Navigational Class&quot; end
    
  def before_new() @base_classes = NavClass.find_all(nil, &quot;name&quot;) end
  def before_edit() @base_classes = NavClass.find_all([ &quot;id != ?&quot;, @nav_class.id], &quot;name&quot;) end
  def list_order() &quot;name&quot; end  


  def before_delete() ModelObserver.suspend end
  def after_delete() ModelObserver.resume end 
  
  def notice_for_create() &quot;Class &apos;#{@nav_class.name}&apos; created. Now, add the attributes and/or links.&quot; end
  def notice_for_update() &quot;Class &apos;#{@nav_class.name}&apos; updated.&quot; end
  def notice_for_delete() &quot;Class &apos;#{@nav_class.name}&apos; deleted.&quot; end

  def redirect_after_create() redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @nav_class.id end

  def prepare_for_edit_attribute
    @data_types = (NavAttribute.data_types.collect { |dt| [dt, dt] })
    @contexts = Context.find_all nil, &quot;name&quot;
    @indices = Index.find_all nil, &quot;name&quot;
    @links = Link.find_all nil, &quot;name&quot;
  end
  
  def new_attribute(after_error = false)
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;]) unless after_error
    @nav_attribute = NavAttribute.new(&quot;nav_class&quot; =&gt; @nav_class) unless after_error
    prepare_for_edit_attribute
    @page_title = &quot;New Attribute for #{@nav_class.name}&quot;
    render_action &quot;edit_attribute&quot;
  end       

  def edit_attribute(after_error = false)
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;]) unless after_error
    @nav_attribute = NavAttribute.find(@params[&quot;id&quot;]) unless after_error
    prepare_for_edit_attribute
    @page_title = &quot;Edit Attribute for #{@nav_class.name}&quot;
    render_action &quot;edit_attribute&quot;
  end       
  
  def create_attribute
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;])
    @nav_attribute = NavAttribute.new(@params[&quot;nav_attribute&quot;])
    if @nav_attribute.save
      flash[&quot;notice&quot;] = &quot;Attribute &apos;#{@nav_attribute.name}&apos; added.&quot;
      redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @params[&quot;nav_class_id&quot;]
    else
      treat_errors @nav_attribute, &quot;new_attribute&quot;
    end
  end

  def update_attribute
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;])
    @nav_attribute = NavAttribute.find(@params[&quot;nav_attribute&quot;][&quot;id&quot;])
    @nav_attribute.attributes = @params[&quot;nav_attribute&quot;]
    if @nav_attribute.save
      flash[&quot;notice&quot;] = &quot;Attribute &apos;#{@nav_attribute.name}&apos; updated.&quot;
      redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @params[&quot;nav_class_id&quot;]
    else
      treat_errors @nav_attribute, &quot;edit_attribute&quot;
    end
  end
    
  def delete_attribute
    a = NavAttribute.find(@params[&quot;id&quot;])
    a.destroy
    flash[&quot;notice&quot;] = &quot;Attribute &apos;#{a.name}&apos; removed.&quot;
    redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @params[&quot;nav_class_id&quot;]
  end

  def new_operation(after_error = false)
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;]) unless after_error
    @nav_operation = NavOperation.new(&quot;nav_class&quot; =&gt; @nav_class) unless after_error
    @page_title = &quot;New Operation for #{@nav_class.name}&quot;
    render_action &quot;edit_operation&quot;
  end
  
  def edit_operation(after_error = false)
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;]) unless after_error
    @nav_operation = NavOperation.find(@params[&quot;id&quot;]) unless after_error
