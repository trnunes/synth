<rdf:Description rdf:about="http://sr#o_3cf0287f">
	<sr:code>current_password = params[&quot;current_password&quot;]
if request.post?
new_password = params[&quot;new_password&quot;]
verify_password = params[&quot;verify_password&quot;]
user = session[:user]
login = user.login

flash[:notice] = &quot;&quot;
flash[:error] = &quot;&quot;
user = authenticate(login, current_password)
if user
	if request.post? and (new_password== verify_password)
		 user.set_password(@context, nil, new_password) 
		 user.save
		 flash[:notice] = &quot;Your password has been changed.&quot;
	else
		 flash[:error] = &quot;Passwords don&apos;t match&quot;
	end
else
	flash[:error] = &quot;The current password you&apos;ve entered is incorrect. Please enter a different password.&quot; 
end
end
render :inline =&gt; View.find_by_name(&apos;ChangePassword&apos;).template</sr:code>
</rdf:Description>

<rdf:Description rdf:about="http://sr#o_95ffd4bd">
	<sr:template>&lt;script language=&quot;javascript&quot;&gt;

// Define a custom format function
addAttchmentField = function() {
  var attSize = document.getElementById(&quot;attachments_fields_size&quot;);
  var attachments = document.getElementById(&quot;attachments&quot;);
  var attField = document.createElement(&apos;input&apos;);
  attField.name = &quot;node[attachment_&quot; + attSize.value + &quot;]&quot;;
  var newLine = document.createElement(&apos;br&apos;);
  attField.type = &quot;file&quot;;
  attachments.appendChild(newLine);
  attachments.appendChild(attField);
  attSize.value = parseInt(attSize.value) + 1
}

&lt;/script&gt;


&lt;div align=&quot;right&quot; id=&quot;attachments&quot;&gt;
  &lt;div style=&quot;text-align: center;&quot;&gt; 
    Attachments 
    [&lt;a href=&quot;#&quot; onclick=&quot;JavaScript: addAttchmentField()&quot;&gt;+&lt;/a&gt;]
  &lt;/div&gt;
  &lt;% if @node.get_attachments.size &gt; 0 %&gt;
    &lt;table&gt;
      &lt;tr&gt;&lt;th&gt;remove&lt;/th&gt;&lt;th&gt;name&lt;/th&gt;&lt;/tr&gt;
    &lt;% 
      attachments_size = 0
      @node.get_attachments.each{ |attach| 
    %&gt;
      &lt;tr&gt;
        &lt;td align=&quot;center&quot;&gt;
          &lt;input type=&quot;checkbox&quot; name=&quot;remove_attachment_&lt;%= attachments_size %&gt;&quot; value=&quot;&lt;%= attach.id %&gt;&quot;/&gt;
        &lt;/td&gt;
        &lt;td&gt;&lt;a href=&quot;&lt;%= attach.link %&gt;&quot;&gt;&lt;%= attach.name %&gt;&lt;/a&gt;&lt;/td&gt;
      &lt;/tr&gt;
    &lt;%
