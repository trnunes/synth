<rdf:Description rdf:about="http://sr#o_0b771cdc">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>get_annotations</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>#~ text = &lt;&lt;BLOCK
#~ A cidade do [nome:Rio de Janeiro] é famosa. Nela fica a [praia de Ipanema].
#~ é a capital do seu [estado|capitalDe:Rio de Janeiro]. A populacao é de
#~ aproximadamente [10 milhões|populacao:=10000000] de habitantes. Veja
#~ o [site|Brasil] sobre seu pais.

#~ [a|b:=c]
#~ [a|b:c]
#~ [ab:c]
#~ [a|bc]
#~ [abc]

#~ BLOCK

local_text = text ? text : &quot;&quot;
annotations = Array.new()
annotMatches = local_text.scan(/(\[([^\]]*)\])/)

name1_idx = 0
alias_token_idx = 1
name2_idx = 2
predicate_token_idx = 4
name3_idx = 7

annotMatches.each { |annotMatch| 
#  p annotMatch[1]
  annotParts = annotMatch[1].scan(/([^\|\:]*)(\|?)([^\:]*)(((\:\=)|(\:))?)(.*)/)[0]
#  p annotParts
  annotation = Hash.new()
  annotations &lt;&lt; annotation
  
  
  if !annotParts[alias_token_idx].to_s.empty? 
    aliased = true
  end
  if !annotParts[predicate_token_idx].to_s.empty? 
    predicated = true
  end
  # default values
  annotation[:match] = annotMatch[0]
  annotation[:propertyName] = &quot;WikiSeeAlso&quot;
  annotation[:objectProperty] = true
  if !aliased and !predicated
    annotation[:alias] = annotParts[name1_idx]
    annotation[:value] = annotParts[name1_idx]
  elsif !aliased and predicated
    annotation[:alias] = annotParts[name3_idx]
    annotation[:propertyName] = annotParts[name1_idx]
    annotation[:value] = annotParts[name3_idx]
  elsif aliased and !predicated
    annotation[:alias] = annotParts[name1_idx]
    annotation[:value] = annotParts[name2_idx]
  elsif aliased and predicated
    annotation[:alias] = annotParts[name1_idx]
    annotation[:propertyName] = annotParts[name2_idx]
    annotation[:value] = annotParts[name3_idx]
  end
  if predicated and annotParts[predicate_token_idx] == &quot;:=&quot;
    annotation[:objectProperty] = false
  end
  
  if annotation[:objectProperty]
    pageValue = WikiPage.find_by_wikiLabel(annotation[:value])
    annotation[:value] = pageValue if !pageValue.nil?
  end
  
#  p annotation
}

annotations</sr:code>
	<sr:params>text = nil</sr:params>
	<sr:position>0</sr:position>
</rdf:Description>

