<rdf:Description rdf:about="http://sr#o_39bc397f">
	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>render_query</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>queryMatches = text.scan(/(\{\?([^\}]*)\})/)

queryMatches.each { |queryMatch|
  match = queryMatch[0]
  query = queryMatch[1].to_s
  evaluation = eval(query)
  textualValue = self.render(evaluation)
  text = text.gsub(match, textualValue)
}

text</sr:code>
