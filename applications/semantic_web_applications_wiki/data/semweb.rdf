    else
      treat_errors @nav_operation, &quot;new_operation&quot;
    end
  end 
  
  def update_operation
    @nav_class = NavClass.find(@params[&quot;nav_class_id&quot;])
    @nav_operation = NavOperation.find(@params[&quot;nav_operation&quot;][&quot;id&quot;])
    @nav_operation.attributes = @params[&quot;nav_operation&quot;]
    if @nav_operation.save
