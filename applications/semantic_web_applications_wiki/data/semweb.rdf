	<rdf:type rdf:resource="http://sr#NavOperation"/>
	<sr:name>get_autocomplete_list</sr:name>
	<sr:nav_class_id rdf:resource="http://sr#o_70cb7ac3"/>
	<sr:code>autocompleteList = []
thisKlass = self.class

Link.find_all.each{|ln| 
  if thisKlass &lt;= eval(NavClass.find(ln.nav_class_id).name) and 
     eval(NavClass.find(ln.target_nav_class_id).name) &lt;= AbstractWikiPage then
    autocompleteList &lt;&lt; (&quot;[&quot; &lt;&lt; ln.name &lt;&lt; &quot;:&quot;)
  end
}

reservedAttrs = {&quot;updated_at&quot; =&gt; true, 
                 &quot;id&quot; =&gt; true, 
                 &quot;wikiText&quot; =&gt; true, 
                 &quot;wikiLabel&quot; =&gt; true, 
                 &quot;created_at&quot; =&gt; true 
}

navClass = NavClass.find_by_name(thisKlass.name)
while navClass do
  NavAttribute.find_all_by_nav_class_id(navClass.id).each{ |navAtt|
    if !reservedAttrs[navAtt.name] then
      if navAtt.class == NavAttribute then
        autocompleteList &lt;&lt; (&quot;[&quot; &lt;&lt; navAtt.name &lt;&lt; &quot;:=&quot;)
      end
      autocompleteList &lt;&lt; (&quot;{?&quot; &lt;&lt; navAtt.name &lt;&lt; &quot;}&quot;)
    end
  }
  if navClass.parent_nav_class_id then
    navClass = NavClass.find(navClass.parent_nav_class_id)
  else
    navClass = nil
  end
end

autocompleteList.compact</sr:code>
	<sr:position>0</sr:position>
</rdf:Description>

