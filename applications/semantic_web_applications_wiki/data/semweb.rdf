    o = NavOperation.find(@params[&quot;id&quot;])
    o.destroy
    flash[&quot;notice&quot;] = &quot;Operation &apos;#{o.name}&apos; removed.&quot;
    redirect_to :action =&gt; &quot;edit&quot;, :id =&gt; @params[&quot;nav_class_id&quot;]
